<Window x:Class="Opgave_S2_01_part_11.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Opgave_S2_01_part_11"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <Canvas x:Name="currentImage" >
        <Canvas.Background>
            <ImageBrush x:Name="test" ImageSource="1.png"/>
        </Canvas.Background>

        <StackPanel Orientation="Horizontal" Margin="325 380">


        <Button x:Name="btnPrev" Content="Previous" Canvas.Left="327" Canvas.Top="381" Width="75" Margin="0 0 5 0" Click="btnPrev_Click"/>
        <Button x:Name="btnNext" Content="Next" Canvas.Left="407" Canvas.Top="381" Width="75" Margin="5 0 0 0" Click="btnNext_Click"/>

        </StackPanel>

    </Canvas>


</Window>
